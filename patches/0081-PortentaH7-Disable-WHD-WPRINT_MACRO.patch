@@ -102,19 +102,8 @@ typedef struct
     void *empty;
 } cyhal_spi_t;
 
-
-#include "whd_debug.h"
-
-#if defined(WPRINT_ENABLE_WHD_ERROR) || defined(WPRINT_ENABLE_WHD_INFO) || defined(WPRINT_ENABLE_WHD_DEBUG)
